{
  "name": "GGFRC Population",
  "version": "1.0",
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "*://*/*"
  ],
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "scripting",
    "storage",
    "unlimitedStorage"
  ],
  "action": {
    "default_title": "GGFRC Population",
    "default_popup": "popup.html",
    "default_icon": {
      "32": "icons/32.png",
      "72": "icons/72.png",
      "128": "icons/128.png",
      "512": "icons/512.png"
    }
  },
  "icons": {
    "32": "icons/32.png",
    "72": "icons/72.png",
    "128": "icons/128.png",
    "512": "icons/512.png"
  }
}
